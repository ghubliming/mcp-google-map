  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "author": "CabLate",
  "files": [
    "dist",
    "dist/**/*.map",
    "README.md"
  ],
  "bin": {
    "mcp-google-map": "./dist/index.cjs"
  },
  "keywords": [
    "google",
    "map",
    "api",
    "llm",
    "typescript",
    "mcp"
